{
    "name": "petanque-rating-backend",
    "version": "1.0.0",
    "description": "Backend для системы рейтинга игроков в петанк",
    "main": "dist/server.js",
    "scripts": {
        "dev": "ts-node-dev --respawn --transpile-only src/server.ts",
        "build": "node --max-old-space-size=1024 ./node_modules/.bin/tsc",
        "build:swc": "swc src -d dist --copy-files",
        "start": "node dist/server.js",
        "check-db": "ts-node src/migrations/migrate.ts",
        "test": "jest",
        "test:watch": "jest --watch",
        "test:coverage": "jest --coverage"
    },
    "dependencies": {
        "bcrypt": "^5.1.1",
        "cors": "^2.8.5",
        "dotenv": "^16.3.1",
        "express": "^4.18.2",
        "googleapis": "^160.0.0",
        "helmet": "^7.1.0",
        "jsonwebtoken": "^9.0.2",
        "morgan": "^1.10.0",
        "multer": "^1.4.5-lts.1",
        "mysql2": "^3.6.5",
        "ts-node": "^10.9.1",
        "typescript": "^5.3.2",
        "xlsx": "^0.18.5"
    },
    "devDependencies": {
        "@swc/cli": "^0.1.62",
        "@swc/core": "^1.3.100",
        "@types/bcrypt": "^5.0.2",
        "@types/cors": "^2.8.17",
        "@types/express": "^4.17.21",
        "@types/jest": "^29.5.8",
        "@types/jsonwebtoken": "^9.0.5",
        "@types/morgan": "^1.9.9",
        "@types/multer": "^1.4.11",
        "@types/node": "^20.10.0",
        "jest": "^29.7.0",
        "ts-jest": "^29.1.1",
        "ts-node-dev": "^2.0.0"
    }
}